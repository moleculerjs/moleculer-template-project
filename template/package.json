{
  "name": "{{projectName}}",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services/**/*.service.js",
    "start": "moleculer-runner",
    "cli": "moleculer connect {{transporter}}",
    "ci": "jest --watch",
    "test": "jest --coverage"{{#lint}},
    "lint": "eslint services"{{/lint}}{{#docker}},
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"{{/docker}}
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    {{#lint}}
    "eslint": "^8.6.0",
    {{/lint}}
    "jest": "^27.4.7",
    "jest-cli": "^27.4.7",
    {{#apiGW}}
    "supertest": "^6.2.2",
    {{/apiGW}}
    {{#apiIO}}
    "socket.io-client": "^4.4.1",
    {{/apiIO}}
    {{#apiGQL}}
    "graphql-request": "^4.2.0",
    {{/apiGQL}}
    "moleculer-repl": "^0.7.0"
  },
  "dependencies": {
    {{#apiGW}}
    "moleculer-web": "^0.10.4",
    {{/apiGW}}
    {{#dbService}}
    "@moleculer/database": "git+https://github.com/moleculerjs/database.git",
    "nedb": "^1.8.0",
    {{/dbService}}
    {{#if_eq transporter "NATS"}}
    "nats": "^2.4.0",
    {{/if_eq}}
    {{#if_eq transporter "MQTT"}}
    "mqtt": "^4.2.8",
    {{/if_eq}}
    {{#if_eq transporter "AMQP"}}
    "amqplib": "^0.7.0",
    {{/if_eq}}
    {{#if_eq transporter "AMQP10"}}
    "rhea-promise": "^2.1.0",
    {{/if_eq}}
    {{#if_eq transporter "STAN"}}
    "node-nats-streaming": "^0.3.2",
    {{/if_eq}}
    {{#if_eq transporter "Kafka"}}
    "kafka-node": "^5.0.0",
    {{/if_eq}}
    {{#redis}}
    "ioredis": "^4.28.2",
    {{/redis}}
    {{#apiGQL}}
    "graphql": "^15.8.0",
    "moleculer-apollo-server": "^0.3.6",
    "graphql-type-json": "^0.3.2",
    {{/apiGQL}}
    {{#needChannels}}
    "@moleculer/channels": "^0.1.2",
    {{/needChannels}}
    {{#apiIO}}
    "moleculer-io": "^2.0.0",
    {{/apiIO}}
    "moleculer": "^0.14.19"
  },
  "engines": {
    "node": ">= 12.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
